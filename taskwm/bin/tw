#!/usr/bin/env python3
"""tw - taskwm CLI wrapper."""

import sys
import os

# Add parent directory to path to find taskwm package
script_dir = os.path.dirname(os.path.realpath(__file__))
parent_dir = os.path.dirname(script_dir)
sys.path.insert(0, parent_dir)

from taskwm.cli import main

if __name__ == '__main__':
    sys.exit(main())
